{
    "nodes": [
        "ws://127.0.0.1:41420"
    ],
    "modules": [
        "vpow",
        "caching",
        "rate_limits",
        "heartbeat"
    ],
    "vpow_api_key": ":3",
    "rate_limits": {
        "global": [1000, "1m"],
        "vpow": [30, "24h"]
    },
    
    "port": 3130,
    "host": "127.0.0.1",

    "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST",
        "Access-Control-Allow-Headers": "Content-Type"
    }
}